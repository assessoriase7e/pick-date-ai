generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model appointments {
  id            BigInt   @id @default(autoincrement())
  phone         Decimal? @db.Decimal
  business_name String?
  start_time    String?
  end_time      String?
  event_id      String?
  created_at    DateTime @default(now()) @db.Timestamptz(6)
}

model clients {
  id            BigInt   @id @default(autoincrement())
  full_name     String?
  phone         String?
  created_at    DateTime @default(now()) @db.Timestamptz(6)
  business_name String?
}

model documents {
  id        BigInt                 @id @default(autoincrement())
  content   String?
  metadata  Json?
  embedding Unsupported("vector")?
}
